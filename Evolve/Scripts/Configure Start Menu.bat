@echo off
dism >nul 2>&1 || (echo ^<Run the script as administrator^> && pause>nul && cls&exit)

title Start Menu Configure
color B
mode 45,6

choice /c 12 /n /m "[1] StartAllBack | [2] OpenShell"
if %errorlevel% equ 1 (
    set "_mode=startallback"
)
if %errorlevel% equ 2 (
    set "_mode=openshell"
)

if "%_mode%"=="startallback" (
    echo Replacing Start Menu With StartAllBack...
    taskkill /f /im StartMenuExperienceHost.exe >nul 2>&1
    taskkill /f /im StartMenu.exe >nul 2>&1
    rmdir /s /q "C:\Program Files\Open-Shell" >nul 2>&1
    rmdir /s /q "C:\ProgramData\Microsoft\Windows\Start Menu\Programs\Open-Shell" >nul 2>&1
    rmdir /s /q "C:\Users\Administrator\AppData\Local\OpenShell" >nul 2>&1
    rmdir /s /q "C:\Users\Administrator\AppData\Roaming\OpenShell" >nul 2>&1
    "C:\Windows\Misc\StartAllBackSetup.exe" /silent /allusers >nul 2>&1
    Regedit.exe /s "C:\Windows\Misc\StartAllBack.reg" >nul 2>&1
    rmdir /s /q "C:\Program Files\Open-Shell" >nul 2>&1
    rmdir /s /q "C:\ProgramData\Microsoft\Windows\Start Menu\Programs\Open-Shell" >nul 2>&1
    rmdir /s /q "C:\Users\Administrator\AppData\Local\OpenShell" >nul 2>&1
    rmdir /s /q "C:\Users\Administrator\AppData\Roaming\OpenShell" >nul 2>&1
    start %systemdrive%\Windows\SystemApps\Microsoft.Windows.StartMenuExperienceHost_cw5n1h2txyewy\StartMenuExperienceHost.exe >nul 2>&1
    start %systemdrive%\Windows\SystemApps\MicrosoftWindows.Client.CBS_cw5n1h2txyewy\SearchHost.exe >nul 2>&1
)

